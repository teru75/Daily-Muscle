<div class="comtainer">
  <div class="row">
    <div class="col-xs-8">
      <h1 class="page-header">マイジム</h1>
      <h3>新規登録</h3>
      <%= form_with(model: @my_gym, local: true, url: customer_my_gyms_path) do |f| %>
        <div class="field">
        <%= f.label :name, "名前" %>
        <%= f.text_field :name, class:"form-control" %>
        </div>

        <div class="actions">
          <%= f.submit "登録" %>
        </div>

      <% end %>
    </div>
  </div>
</div>