    <h1 class="page-header">マイジム</h1>
    <div class="col-xs-7">
      <h3>新規登録</h3>
      <%= form_with(model: @my_gym, local: true, url: customer_my_gyms_path) do |f| %>
          <%= render "form", f:f %>
      <% end %>
    </div>
    <%= render "customers/customer_form", customer: @customer %>
  </div>
</div>